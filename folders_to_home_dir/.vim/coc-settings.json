{
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "go.goplsOptions": {
    "completeUnimported": true
    },
    "sourcery": {
      "command": "/home/nisidabay/.local/bin/sourcery",
      "args": [
        "lsp"
      ],
      "filetypes": [
        "python"
      ],
      "initializationOptions": {
        "token": "user_JGeHIJ5887g4LR_xPvS69ASvlBgwH9oGai_kCbzGO8Y2YLHk_Dp_HG5SY_Y",
        "extension_version": "coc.vim",
        "editor_version": "vim"
      },
